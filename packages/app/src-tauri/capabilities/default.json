{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Default permissions for the main window",
  "windows": ["main", "buddy"],
  "permissions": [
    "core:default",
    "shell:allow-open",
    "dialog:allow-open",
    "core:window:allow-show",
    "core:window:allow-set-position",
    "core:window:allow-start-dragging"
  ]
}
